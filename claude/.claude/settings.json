{
  "permissions": {
    "defaultMode": "default"
  },
  "extraKnownMarketplaces": {
    "beads-marketplace": {
      "source": {
        "source": "github",
        "repo": "steveyegge/beads"
      }
    },
    "florin-popa-marketplace": {
      "source": {
        "source": "github",
        "repo": "FlorinPopaCodes/claude-marketplace"
      }
    },
    "ast-grep-marketplace": {
      "source": {
        "source": "github",
        "repo": "ast-grep/claude-skill"
      }
    }
  },
  "hooks": {
    "PreCompact": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "bd prime"
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "bd prime"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline.sh"
  },
  "enabledPlugins": {
    "always-works-testing@florin-popa-marketplace": true,
    "ast-grep@ast-grep-marketplace": true,
    "beads@beads-marketplace": true,
    "code-review@claude-plugins-official": false,
    "commit-commands@claude-plugins-official": true,
    "feature-dev@claude-plugins-official": true,
    "frontend-design@claude-plugins-official": true,
    "learning-output-style@claude-plugins-official": false,
    "plugin-dev@claude-plugins-official": false,
    "security-guidance@claude-plugins-official": true,
    "rails-codegen@florin-popa-marketplace": false,
    "cognitive-load@florin-popa-marketplace": true,
    "simplicity-first@florin-popa-marketplace": true,
    "context7@claude-plugins-official": true,
    "typescript-lsp@claude-plugins-official": true
  },
  "alwaysThinkingEnabled": true
}
